{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",

    "esModuleInterop": true,
    "lib": ["es5", "es6", "dom"]
  },
  "include": [
    "src/index.tsx"
  ]
}
